LazySegmentTreeL seg = LazySegmentTreeL(n, [](ll a, ll b){return a + b;},
                                        0, [](ll a, ll b){return b;},
                                        NAN, [](ll a, ll b, int c){return b * c;},
                                        vector<ll>(n, DEF));
